{
  "private": true,
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "run-s validate build:rollup build:custom-properties build:html build:panel-options",
    "build:rollup": "rollup -c rollup.config.js ",
    "build:custom-properties": "node --experimental-json-modules ./scripts/customProperties.js",
    "build:html": "node ./scripts/html.js",
    "build:panel-options": "node ./scripts/panelOptions.js",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "dev": "vite",
    "fix": "run-s fix:eslint fix:prettier",
    "fix:eslint": "eslint --fix .",
    "fix:prettier": "prettier --write .",
    "lint": "run-s lint:eslint lint:prettier",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "validate": "run-s check lint"
  },
  "devDependencies": {
    "@grafana/data": "~9.0.5",
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.3",
    "@sveltejs/vite-plugin-svelte": "^1.0.1",
    "@tsconfig/svelte": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.3.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "eslint-plugin-unicorn": "^43.0.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.0",
    "rollup": "^2.77.1",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.49.0",
    "svelte-check": "^2.8.0",
    "svelte-preprocess": "^4.10.7",
    "typescript": "^4.7.4",
    "vite": "^3.0.3"
  }
}
